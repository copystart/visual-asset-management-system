{
	"name": "vams",
	"env": {
		"account": null,
		"region": null
	},
	"app": {
		"baseStackName": "prod",
		"stagingBucketName": null,
		"adminEmailAddress": "adminEmail@example.com",
		"useFIPS": true,
		"govCloud": {
			"enabled": true
		},
		"openSearchServerless": {
			"enabled": false
		},
		"locationService": {
			"enabled": false
		},
		"albDeploy": {
			"enabled": true,
			"publicSubnet": false,
			"vpcCidrRange": "10.1.0.0/16",
			"domainHost": "vams1.example.com",
			"certificateARN": "arn:aws-us-gov:acm:<REGION>:<ACCOUNTID>:certificate/<CERTIFICATEID>",
			"optionalHostedZoneID": ""
		},
		"authProvider": {
			"cognito": {
				"enabled": true,
				"samlEnabled": false
			}
		}
	}
}
