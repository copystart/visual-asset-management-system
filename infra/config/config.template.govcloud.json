{
	"name": "vams",
	"env": {
		"account": null,
		"region": null
	},
	"app": {
		"baseStackName": "prod",
		"stagingBucketName": null,
		"adminEmailAddress": "adminEmail@example.com",
		"useFips": true,
		"govCloud": {
			"enabled": true
		},
		"useOpenSearchServerless": {
			"enabled": false
		},
		"useLocationService": {
			"enabled": false
		},
		"useAlb": {
			"enabled": true,
			"publicSubnet": false,
			"vpcCidrRange": "10.1.0.0/16",
			"domainHost": "vams1.example.com",
			"certificateARN": "arn:aws-us-gov:acm:<REGION>:<ACCOUNTID>:certificate/<CERTIFICATEID>",
			"optionalHostedZoneID": ""
		},
		"pipelines": {
			"usePointCloudVisualization": {
				"enabled": false
			}
		},
		"authProvider": {
			"useCognito": {
				"enabled": true,
				"useSaml": false
			}
		}
	}
}
